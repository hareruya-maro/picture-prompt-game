{
  "apphosting": {
    "backendId": "pic-pro-front",
    "rootDir": "pic-pro-front",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ],
    "alwaysDeployFromSource": true
  },
  "firestore": {
    "rules": "firestore.rules"
  },
  "storage": {
    "rules": "storage.rules",
    "bucket": "picture-prompt-game.firebasestorage.app"
  },
  "emulators": {
    "apphosting": {
      "port": 5002,
      "rootDirectory": "./pic-pro-front",
      "startCommand": "npm run dev"
    },
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8085
    },
    "storage": {
      "port": 9199,
      "rules": "storage.rules.dev"
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "firebase-debug.log",
        "firebase-debug.*.log",
        ".runtimeconfig.json"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ]
}