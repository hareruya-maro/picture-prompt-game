{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "genkit:start": "genkit start -- tsx --watch src/genkit-sample.ts",
    "dev": "firebase emulators:start --only  apphosting,functions,firestore,auth,storage --import=./emulator-data --export-on-exit"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@genkit-ai/firebase": "^1.16.1",
    "@genkit-ai/googleai": "^1.16.1",
    "@genkit-ai/vertexai": "^1.16.1",
    "express": "^5.1.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "genkit": "^1.16.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^3.1.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2"
  },
  "private": true
}